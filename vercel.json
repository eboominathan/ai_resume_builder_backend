{
    "version": 2,
    "public": "public",
    "builds": [
      {
        "src": "api/index.php",
        "use": "@vercel/php"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "public/index.php"
      }
    ]
  }
  